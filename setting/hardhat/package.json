{
  "name": "@layerzerolabs/hardhat-config",
  "version": "1.5.33",
  "private": true,
  "description": "LayerZero Hardhat Config",
  "keywords": [
    "hardhat",
    "config"
  ],
  "license": "MIT",
  "files": [
    "index.ts",
    "hardhat.config.ts"
  ],
  "scripts": {
    "build": "$npm_execpath tsc",
    "clean": "$npm_execpath rimraf node_modules",
    "format": "$npm_execpath prettier --ignore-path $(git rev-parse --show-toplevel)/.prettierignore --write .",
    "lint": "$npm_execpath eslint --no-error-on-unmatched-pattern . --ext .ts --fix"
  },
  "resolutions": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13"
  },
  "devDependencies": {
    "@layerzerolabs-internal/typescript-config": "workspace:^",
    "@layerzerolabs/hardhat-deploy-mnemonic": "workspace:^",
    "@layerzerolabs/lz-definitions": "workspace:^",
    "@layerzerolabs/monorepo-utilities": "workspace:^",
    "@layerzerolabs/runtime-config": "workspace:^",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.6",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.19",
    "chai": "^4.3.7",
    "chai-ethers": "^0.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.16.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.11.22",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-spdx-license-identifier": "^2.1.0",
    "js-yaml": "^4.1.0",
    "rimraf": "^3.0.2",
    "solidity-coverage": "^0.8.2",
    "typescript": "^5.1.3",
    "web3": "^1.8.2"
  }
}
